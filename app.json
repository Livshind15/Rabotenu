{
  "name": "Rabotenu",
  "displayName": "Rabotenu",
  "expo": {
    "name": "Rabotenu",
    "slug": "rabotenu",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "icon": "./assets/icon.png",
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "ts",
        "tsx",
        "svg"
      ]
    },
    "splash": {
      "image": "./assets/background.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF"
 
 
    },
    "ios": {
      "splash": {
        "image": "./assets/background.png",
        "resizeMode": "cover"
      },
      "bundleIdentifier": "org.markup.exp"
    },
    "android": {
	      "package": "rabotenu.livshin.com",
      "splash": {
        "mdpi": "./assets/background.png",
        "hdpi": "./assets/background.png",
        "xhdpi": "./assets/background.png",
        "xxhdpi": "./assets/background.png",
        "xxxhdpi": "./assets/background.png",
        "resizeMode": "cover"
      }
    }
  }
}
