{
  "name": "Rabotenu",
  "displayName": "רבותינו",
  "expo": {
    "name": "Rabotenu",
    "slug": "rabotenu",
    "version": "1.0.0",
    "platforms": ["ios", "android", "web"],
    "assetBundlePatterns": ["**/*"],

    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "splash": {
      "image": "./assets/background.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "splash": {
        "image": "./assets/background.png",
        "resizeMode": "cover"
      },
      "icon": "./assets/icon.png",

      "bundleIdentifier": "org.markup.exp"
    },
    "android": {
      "package": "rabotenu.livshin.com",
      "icon": "./assets/icon.png",

      "splash": {
        "mdpi": "./assets/background.png",
        "hdpi": "./assets/background.png",
        "xhdpi": "./assets/background.png",
        "xxhdpi": "./assets/background.png",
        "xxxhdpi": "./assets/background.png",
        "resizeMode": "cover"
      }
    }
  }
}
