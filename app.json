{
  "name": "רבותינו",
  "displayName": "רבותינו",
  "expo": {
    "name": "רבותינו",
    "slug": "Rabotenu",
    "version": "1.0.0",
    "platforms": ["ios", "android", "web"],
    "assetBundlePatterns": ["**/*"],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "packagerOpts": {
      "config": "metro.config.js",

      "sourceExts": ["svg"]
    },
    "splash": {
      "image": "./assets/background.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF"
    },

    "web": {
      "name": "רבותינו | עולם חדש של דיוק",
      "shortName": "רבותינו",
      "favicon": "./assets/icon.png",

      "backgroundColor": "#f00",
      "lang": "he"
    },
    "ios": {
      "splash": {
        "image": "./assets/background.png",
        "resizeMode": "cover",
        "supportsTablet": true

      },
      "icon": "./assets/icon.png",

      "bundleIdentifier": "org.markup.exp"
    },
    "android": {
      "package": "rabotenu.livshin.com",
      "icon": "./assets/icon.png",

      "splash": {
        "mdpi": "./assets/background.png",
        "hdpi": "./assets/background.png",
        "xhdpi": "./assets/background.png",
        "xxhdpi": "./assets/background.png",
        "xxxhdpi": "./assets/background.png",
        "resizeMode": "cover"
      }
    }
  }
}
